-- Initialize and return
local success, result = pcall(function()
    return FemboyHub:Init()
end)

if not success then
    warn("Femboy Hub Error: " .. tostring(result))
    -- Try again with PlayerGui
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "FemboyHubError"
    ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")
    
    local ErrorFrame = Instance.new("Frame")
    ErrorFrame.Size = UDim2.new(0, 300, 0, 150)
    ErrorFrame.Position = UDim2.new(0.5, -150, 0.5, -75)
    ErrorFrame.BackgroundColor3 = Color3.fromRGB(230, 215, 225)
    ErrorFrame.Parent = ScreenGui
    
    local ErrorCorner = Instance.new("UICorner")
    ErrorCorner.CornerRadius = UDim.new(0, 15)
    ErrorCorner.Parent = ErrorFrame
    
    local ErrorText = Instance.new("TextLabel")
    ErrorText.Size = UDim2.new(1, -20, 1, -20)
    ErrorText.Position = UDim2.new(0, 10, 0, 10)
    ErrorText.BackgroundTransparency = 1
    ErrorText.Text = "Error loading Femboy Hub!\n\n" .. tostring(result) .. "\n\nCheck console for details."
    ErrorText.TextColor3 = Color3.fromRGB(200, 100, 150)
    ErrorText.Font = Enum.Font.Gotham
    ErrorText.TextSize = 14
    ErrorText.TextWrapped = true
    ErrorText.Parent = ErrorFrame
end

return FemboyHub
